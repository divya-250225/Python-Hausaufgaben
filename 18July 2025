a = 1
while a <= 100:
    print(a)
    a += 1
print()
for b in range(2, 51, 2):
    print(b)
print()
correct_password = "divi@2502"
password = ""
while password != correct_password:
    password = input("Enter password: ")
print("Access granted.")
